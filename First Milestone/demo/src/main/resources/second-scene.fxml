<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" stylesheets="@org/example/demo/styles.css" xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo.SecondController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Button fx:id="runButton" styleClass="action-button,run-button" mnemonicParsing="false" onAction="#onRunButtonClick"
            text="Run">
        <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="stopButton" styleClass="action-button,stop-button" mnemonicParsing="false"
            onAction="#onStopButtonClick" text="Stop">
        <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
            </ImageView>
        </graphic>
    </Button>
</VBox>